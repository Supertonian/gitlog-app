{
  "name": "gitlog-app",
  "version": "1.1.0",
  "description": "Gitlog app with notification",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:win": "electron-packager . gitlog --platform=win32 --overwrite",
    "release": "git add --force dist && standard-version -a"
  },
  "repository": "https://github.com/ZeroStrength/gitlog-app",
  "keywords": [
    "Electron",
    "Git",
    "log"
  ],
  "author": "Changyun Lee",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron-packager": "^15.0.0",
    "prettier": "^2.1.1",
    "standard-version": "^9.0.0"
  },
  "dependencies": {
    "electron": "^9.0.5",
    "gitlog": "^4.0.0",
    "jquery": "^3.5.1",
    "luxon": "^1.25.0",
    "simple-git": "^2.11.0"
  }
}
